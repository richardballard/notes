      "images": "**/*.{png,jpg,jpeg,gif,ico,svg}",
      "fonts": "**/*.{eot,svg,ttf,woff,woff2}"
    },
    "main": {
      "server": "index.js"
    }
  },
  "devDependencies": {
    "lodash": "^4.17.11",
    "minimatch": "^3.0.4",
    "hoek": "^5.0.4",
    "babel-core": "*",
    "babel-preset-env": "*",
    "gulp": "*",
    "gulp-autoprefixer": "*",
    "gulp-babel": "*",
    "gulp-bundle-assets": "*",
    "gulp-clean-css": "*",
    "gulp-purge-sourcemaps": "*",
    "gulp-rename": "*",
    "gulp-sass": "*",
    "gulp-sourcemaps": "*",
    "gulp-strip-comments": "*",
    "gulp-util": "*",
    "lazypipe": "*",
    "require-dir": "*"
  },
  "scripts": {
    "gulp": "gulp"
  }
