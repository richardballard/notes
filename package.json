{
  "version": "1.0.0",
  "name": "notes",
  "private": true,
  "gulp": {
    "src": {
      "js": "content/js/",
      "jsVendors": "content/js/vendors/",
      "scss": "content/scss/",
      "images": "content/img/",
      "dist": "dist/"
    },
    "selectors": {
      "all": "*",
      "js": "**/*.js",
      "css": "**/*.css",
